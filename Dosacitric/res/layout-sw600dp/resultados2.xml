                android:layout_height="30dp"
                android:layout_gravity="center"
                android:background="@drawable/ayuda" />

            <View
                android:layout_width="1dip"
                android:layout_height="fill_parent"
                android:layout_margin="10dp"
                android:background="#ff6600" />

