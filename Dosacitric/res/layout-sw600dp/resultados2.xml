                android:text="INDICE"
                android:textColor="#ff6600" />

            <View
                android:layout_width="1dip"
                android:layout_height="fill_parent"
                android:layout_margin="10dp"
                android:background="#ff6600" />

            <ImageButton
                android:id="@+id/ayuda"
